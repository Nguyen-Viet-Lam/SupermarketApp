<configuration>
  <connectionStrings>
    <!-- 
    Cập nhật Server name phù hợp với máy của bạn
    
    Ví dụ 1 - Local SQL Server Express (mặc định):
    Server=localhost\SQLEXPRESS;Database=SupermarketDB;Trusted_Connection=True;TrustServerCertificate=True
    
    Ví dụ 2 - Local SQL Server (default instance):
    Server=localhost;Database=SupermarketDB;Trusted_Connection=True;TrustServerCertificate=True
    
    Ví dụ 3 - Custom server (lấy tên từ SSMS):
    Server=DESKTOP-ABC123\SQLEXPRESS;Database=SupermarketDB;Trusted_Connection=True;TrustServerCertificate=True
    
    Để tìm tên server của bạn:
    1. Mở SSMS
    2. Chạy: SELECT @@SERVERNAME;
    3. Copy kết quả vào Server=<kết quả>
    -->
    <add name="SupermarketDB" connectionString="Server=localhost\SQLEXPRESS;Database=SupermarketDB;Trusted_Connection=True;TrustServerCertificate=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
